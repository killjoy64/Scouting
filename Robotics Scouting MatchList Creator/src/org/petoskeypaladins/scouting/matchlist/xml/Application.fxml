<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="100.0" prefWidth="350.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.petoskeypaladins.scouting.matchlist.Controller">
   <children>
      <GridPane prefHeight="100.0" prefWidth="350.0">
        <columnConstraints>
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <TextField fx:id="teamID" GridPane.columnIndex="2" GridPane.columnSpan="2" GridPane.rowIndex="1" />
            <TextField fx:id="matchID" GridPane.columnIndex="4" GridPane.rowIndex="1" />
            <Button fx:id="addMatch" mnemonicParsing="false" onAction="#addMatch" prefHeight="25.0" prefWidth="53.0" text="Add" GridPane.columnIndex="6" GridPane.rowIndex="1" />
            <ComboBox fx:id="robotID" prefWidth="150.0" GridPane.columnSpan="2" GridPane.rowIndex="1" />
            <Label text="*Make sure you select an alliance for the matches you are doing!" GridPane.columnSpan="7" />
            <Button fx:id="submit" mnemonicParsing="false" onAction="#createFile" prefHeight="25.0" prefWidth="53.0" text="Finish" GridPane.columnIndex="6" GridPane.rowIndex="2" />
            <Button fx:id="clearAll" alignment="CENTER_RIGHT" contentDisplay="RIGHT" mnemonicParsing="false" onAction="#clearData" text="Clear All" GridPane.columnIndex="4" GridPane.columnSpan="2" GridPane.rowIndex="2">
               <GridPane.margin>
                  <Insets />
               </GridPane.margin></Button>
            <Button mnemonicParsing="false" onAction="#showMasterlistDialog" text="Create Masterlist" GridPane.columnIndex="1" GridPane.columnSpan="3" GridPane.rowIndex="2" />
         </children>
      </GridPane>
   </children>
</AnchorPane>
